{
  "name": "@rythm/bot",
  "version": "1.0.0",
  "private": true,
  "description": "Yet another open-source discord music bot",
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf build && rimraf node_modules && yarn setup",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "build": "tsc -p tsconfig.json",
    "setup": "yarn install && yarn build"
  },
  "repository": "git@github.com:nedanwr/RythmBot.git",
  "author": "Naveed Ali Anwar <hello@naveedalianwar.com>",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.11.0",
    "@distube/yt-dlp": "^1.1.3",
    "discord.js": "^14.1.2",
    "distube": "^4.0.3",
    "libsodium-wrappers": "^0.7.10"
  },
  "devDependencies": {
    "@types/node": "^18.6.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "4.6.4"
  }
}
